        for i, (rok, year_value) in enumerate(zip(lata, values)):
            ax = self.figure.add_subplot(1, cols, i + 1)
            print(range(len(countries)))
            print(len(year_value))
            bars = ax.bar(
                x = range(len(countries)),
                height =year_value,
                color=[colors[region] for region in countries],
                width=0.7
            )
        

            # Ukrywamy wszystko zbÄ™dne
            ax.set_xticks([])
            ax.set_ylim(0, max(values))
            if i > 0: ax.set_yticks([])
            for spine in ax.spines.values():
                spine.set_visible(False)